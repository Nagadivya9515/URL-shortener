generator client {
  provider = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {

  short_code  String    @id @db.VarChar(8)

  target_url  String

  total_clicks Int      @default(0)

  created_at  DateTime  @default(now())
  
  last_clicked DateTime?

  @@map("links")
}